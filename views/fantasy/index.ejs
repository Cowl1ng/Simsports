<%- include('_headers.ejs') %>
<%- include('_messages') %>
<br>

<% fantasygames.forEach(fantasygame => {  %>
  <div id="container">
    <a href="/fantasy/<%= fantasygame.id %>"><%= fantasygame.team_a %> vs <%= fantasygame.team_b %></a>
  <p class="lead mb-3"><%= fantasygame.team_a %> to win: <%= fantasygame.odds_a %></p>
  <p class="lead mb-3"><%= fantasygame.team_b %> to win: <%= fantasygame.odds_b %></p>
  <p class="lead mb-3">Draw : <%= fantasygame.odds_draw %></p>
  <a href="/fantasy/<%= fantasygame.id %>/edit">Edit</a>
  </div>
 <% }) %>
